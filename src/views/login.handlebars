<article>
    <h1>Inicar sesion</h1>
    <form action="/api/sessions/login" method="post">
        <input id="email-input" type="email" name="email" placeholder="Correo" required>
        <input id="password-input" type="password" name="password" placeholder="Contraseña" required>
        <a href="/recovery-password">Olvide mi contraseña</a>
        <input id="submit-button" type="submit" value="Ingresar">
        <br>
        <a href="/register">Registrar nuevo usuario</a>
    </form>
</article>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.min.js"></script>
<script type="module" src="js/login.public.js"></script>